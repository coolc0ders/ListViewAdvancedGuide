<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ListViewCompleteGuide.MasterDetailNav"
             xmlns:pages="clr-namespace:ListViewCompleteGuide"
                  MasterBehavior="Popover">
  <MasterDetailPage.Master>
    <pages:MasterDetailNavMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
                <pages:ContactsPage />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>